<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">

	<context:annotation-config />
	
	<bean id="xxxxx" class="com.spring.Cat">
		<property name="catName" value="야용이" />
		<property name="catAge" value="3" />
	</bean>
	
	<bean id="xx" class="com.spring.Cat" autowire-candidate="false">
		<property name="catName" value="나비" />
		<property name="catAge" value="1" />
	</bean>
	
	<bean id="onePerson" class="com.spring.Person" autowire="byType">
		<property name="username" value="박세진" />
		<!-- cat이 자동으로 주입된다 -->
	</bean>
</beans>
