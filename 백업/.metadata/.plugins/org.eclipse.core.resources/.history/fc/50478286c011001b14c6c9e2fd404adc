package com.dto;

public class Person {
	private String name;
	private int age;
	private String gender;
	private Cat cat;
	public Person(String name, int age, String gender, Cat cat) {
		super();
		this.name = name;
		this.age = age;
		this.gender = gender;
		this.cat = cat;
	}
	@Override
	public String toString() {
		return "Person [name=" + name + ", age=" + age + ", gender=" + gender + ", cat=" + cat + "]";
	}
	
	
	
	public String getCatName() {
		return cat.getCatName();
	}
}
